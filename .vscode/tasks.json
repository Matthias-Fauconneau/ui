{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": ["--manifest-path","examples/empty/Cargo.toml"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "Empty",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": true
			}
		}
	]
}